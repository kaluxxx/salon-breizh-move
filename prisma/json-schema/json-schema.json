{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "role": {
          "type": "string",
          "default": "USER",
          "enum": [
            "ADMIN",
            "MODERATOR",
            "USER"
          ]
        },
        "emailVerified": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "accounts": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Account"
          }
        },
        "sessions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Session"
          }
        }
      },
      "required": [
        "email"
      ]
    },
    "Account": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "provider": {
          "type": "string"
        },
        "providerAccountId": {
          "type": "string"
        },
        "refresh_token": {
          "type": [
            "string",
            "null"
          ]
        },
        "access_token": {
          "type": [
            "string",
            "null"
          ]
        },
        "expires_at": {
          "type": [
            "integer",
            "null"
          ]
        },
        "token_type": {
          "type": [
            "string",
            "null"
          ]
        },
        "scope": {
          "type": [
            "string",
            "null"
          ]
        },
        "id_token": {
          "type": [
            "string",
            "null"
          ]
        },
        "session_state": {
          "type": [
            "string",
            "null"
          ]
        },
        "user": {
          "$ref": "#/definitions/User"
        }
      },
      "required": [
        "userId",
        "type",
        "provider",
        "providerAccountId"
      ]
    },
    "Session": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "sessionToken": {
          "type": "string"
        },
        "expires": {
          "type": "string",
          "format": "date-time"
        },
        "user": {
          "$ref": "#/definitions/User"
        }
      },
      "required": [
        "sessionToken",
        "userId",
        "expires"
      ]
    },
    "VerificationToken": {
      "type": "object",
      "properties": {
        "identifier": {
          "type": "string"
        },
        "token": {
          "type": "string"
        },
        "expires": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "identifier",
        "token",
        "expires"
      ]
    },
    "QuoteRequest": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "company": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "function": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "company",
        "name",
        "function",
        "email",
        "phone",
        "updatedAt"
      ]
    },
    "BadgeRequest": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "company": {
          "type": "string"
        },
        "jobTitle": {
          "type": "string"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "name",
        "email",
        "company",
        "jobTitle",
        "updatedAt"
      ]
    },
    "Contact": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "message": {
          "type": "string"
        }
      },
      "required": [
        "firstName",
        "lastName",
        "email",
        "message"
      ]
    }
  },
  "type": "object",
  "properties": {
    "user": {
      "$ref": "#/definitions/User"
    },
    "account": {
      "$ref": "#/definitions/Account"
    },
    "session": {
      "$ref": "#/definitions/Session"
    },
    "verificationToken": {
      "$ref": "#/definitions/VerificationToken"
    },
    "quoteRequest": {
      "$ref": "#/definitions/QuoteRequest"
    },
    "badgeRequest": {
      "$ref": "#/definitions/BadgeRequest"
    },
    "contact": {
      "$ref": "#/definitions/Contact"
    }
  }
}